{
  "env": {
    "development": {
      "plugins": ["@babel/plugin-transform-react-jsx-source"]
    },
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ],
  "presets": [
    "@babel/preset-flow",
    ["@babel/env", { "modules": false }],
    "@babel/preset-react"
  ]
}